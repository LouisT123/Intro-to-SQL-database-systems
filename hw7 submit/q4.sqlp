USE geo;
SELECT t.`#text` as ethnic_group, count(*) as num_countries, 
	SUM(float(t.`-percentage`) * int(Y.population)) as total_population
FROM world AS X, X.mondial.country AS Y, 
                   CASE WHEN Y.ethnicgroups is missing 
		   THEN []
                   WHEN is_array(Y.ethnicgroups)
		   THEN Y.ethnicgroups
                   ELSE [Y.ethnicgroups] END t
GROUP BY t.`#text`;


